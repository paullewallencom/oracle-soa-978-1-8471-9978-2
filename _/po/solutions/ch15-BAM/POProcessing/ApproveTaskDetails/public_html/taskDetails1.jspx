                                    columns="#{bindings.cardNumber.hints.displayWidth}"
                                    maximumLength="#{bindings.cardNumber.hints.precision}"
                                    shortDesc="#{bindings.cardNumber.hints.tooltip}"
                                    id="it10">
                        <f:validator binding="#{bindings.cardNumber.validator}"/>
                      </af:inputText>
                      <af:inputText value="#{bindings.cardType.inputValue}"
