                                    required="#{bindings.cardType.hints.mandatory}"
                                    columns="#{bindings.cardType.hints.displayWidth}"
                                    maximumLength="#{bindings.cardType.hints.precision}"
                                    shortDesc="#{bindings.cardType.hints.tooltip}"
                                    id="it1">
                        <f:validator binding="#{bindings.cardType.validator}"/>
                      </af:inputText>
                    </af:panelFormLayout>
                  </af:panelGroupLayout>
                </af:panelGroupLayout>
              </af:showDetailHeader>
              <!-- use "choose" to control history visibility. Do not use
                             "visible" or "rendered" flag -->
              <c:choose>
                <c:when test="${actionAvailable.showHistory}">
                  <af:showDetailHeader size="2" id="histHd"
