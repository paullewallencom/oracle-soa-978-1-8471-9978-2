                                    id="attFile"
                                    visible="#{pageFlowScope.readAttachmentBean.selectedAttachmentType == 'file'}"
                                    required="#{pageFlowScope.readAttachmentBean.selectedAttachmentType == 'file'}"
                                    partialTriggers="edAttTy"/>
                    </af:panelFormLayout>
                  </af:panelGroupLayout>
