                                  shortDesc="#{bindings.status.hints.tooltip}"
                                  id="it7">
                      <f:validator binding="#{bindings.status.validator}"/>
                    </af:inputText>
                  </af:panelFormLayout>
                  <af:panelGroupLayout layout="vertical"
